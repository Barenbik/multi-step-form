{
    "version": 2,
    "builds": [
      {
        "src": "dist/**/*",
        "use": "@vercel/vue"
      }
    ],
    "routes": [
      {
        "src": "/assets/images/.*",
        "dest": "/assets/images/index.html"
      },
      {
        "src": "/assets/images/(.*)",
        "dest": "/assets/images/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  